<%include ../common/header.ejs%>
  <style>
    ul {
      list-style-type: none;
      margin: 0;
      padding: 0;
      overflow: hidden;
      background-color: #333;
    }

    li {
      float: left;
    }

    li a {
      display: block;
      color: white;
      text-align: center;
      padding: 14px 16px;
      text-decoration: none;
    }

    li a:hover {
      background-color: #111;
    }

    #followers img {
      width: 40%;
    }
  </style>
  <ul>
    <li>
      <a class="active" href="#home">Home</a>
    </li>
    <li>
      <a href="#news">News</a>
    </li>
    <li>
      <a href="#contact">Contact</a>
    </li>
    <li>
      <a href="#about">About</a>
    </li>
  </ul>

  <div class="row">
    <div class="col-4">
      <div class="card">
        <div class="card-header">
          <span>Social Media Followers</span>
        </div>
        <div class="card-body">
          <div class="row" id="followers" align="center">
            <div class="col-4">
              <img v-bind:src="fb_icon">
              <H2> 4,295 </H2>
            </div>
            <div class="col-4">
              <img v-bind:src="insta_icon">
